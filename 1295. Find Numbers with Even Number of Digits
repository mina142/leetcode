public class EvenNumber {
    public static void main(String[] args){
         int intArray1[] = new int[]{12,345,6785,6,366,78};
         int num , count = 0;
         for(int i = 0 ; i <= 5 ; i++){
             //System.out.println(intArray1[i]);
             num = intArray1[i];
            while (num>0) {
                num = num / 10;
                ++count;
            }
             System.out.println(intArray1[i] + " has " + count + " digits ");
            count = 0;
         }
    }
}
